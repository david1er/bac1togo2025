		<inclure>contacts_pipelines.php</inclure>
	</pipeline>

	<pipeline>
		<nom>optimiser_base_disparus</nom>
		<inclure>contacts_pipelines.php</inclure>
	</pipeline>

	<!-- pour charger les nouvelles balises #PSEUDO et #ORGANISATIONS -->

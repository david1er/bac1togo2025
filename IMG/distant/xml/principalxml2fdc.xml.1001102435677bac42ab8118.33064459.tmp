	<necessite nom="ezcache" compatibilite="[1.2.9;[" />
	<utilise nom="ezrest" compatibilite="[1.2.3;[" />

	<menu nom="boussole" titre="boussole:titre_page_boussole" parent="bando_squelette" icone="images/boussole-16.png" action="boussoles" />
